.TBS5 th,
.TBS5 td{height:30px;}
.TBS5 th{text-align:left;color:#222222;font-weight:bold;font-size:12px;border-right:1px solid #aeaeae;border-bottom:1px solid #aeaeae;}
.TBS5 td{text-align:left;color:#333333;padding:0 10px 0 10px;border-right:1px solid #aeaeae;border-bottom:1px solid #aeaeae;}

.TBS4{width:100%;text-align:left;border-top:2px solid #e66c6c;}
.TBS4 th,
.TBS4 td{height:35px;}
.TBS4 th{text-align:left;background:#eaeaea;color:#222222;font-weight:bold;font-size:12px;border-bottom:1px solid #aeaeae;padding:0 0 0 10px;}
.TBS4 td{text-align:left;color:#333333;font-size:12px;border-bottom:1px solid #aeaeae;padding:0 10px 0 10px;}

.TBS3{width:100%;text-align:center;border-top:2px solid #e66c6c;}
.TBS3 th,
.TBS3 td{height:35px;}
.TBS3 th{background:#eaeaea;color:#222222;font-weight:bold;font-size:12px;border-bottom:1px solid #777777;}
.TBS3 td{color:#333333;font-size:12px;border-bottom:1px solid #aeaeae;}
.TBS3 tr:hover{background:#eee;}
.TBS3 .SelRow{background:#eee;}

.diag_majrSel {
	background:rgb(238, 238, 238);
}