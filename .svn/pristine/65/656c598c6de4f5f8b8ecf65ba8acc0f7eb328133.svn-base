@charset "utf-8";
/* CSS Document */

html,body { margin:0; padding:0; color:#666;}
body {width:100%;height:100%;min-width:980px;color:#666; font-size:12px;font-family:돋움, Dotum, Verdana, sans-serif; -webkit-text-size-adjust:none;}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{ margin:0; padding:0; }
table { border-collapse:collapse; border-spacing:0; }
caption {visibility:hidden; font-size:0; width:0; height:0; line-height:0}

fieldset,img,iframe { border:0; }
button { margin:0; padding:0; border:none; cursor:pointer;vertical-align:top; font-family:돋움, Dotum, Verdana, sans-serif; }
h1,h2,h3,h4,h5,h6{ font-size:100%; font-weight:normal; font-family:돋움, dotum, Verdana, sans-serif; }
h4 {display:none;}
li,dt,dd {list-style:none;}
li,dt,dd,img,p {display:inline;padding:0;margin:0;border:0;}
input,select { vertical-align:middle; }
input,textarea,select{ font-family:inherit; font-size:inherit; font-weight:inherit; }
textarea {resize:none;}
a {text-decoration:none;}

/***layout***/
#header {position:relative;height:60px;background:#e5e5e5;width:100%;min-width:1280px;}
#cnt {position:relative;height:auto;width:100%;min-width:1280px;padding:0;background:#ffffff url('/img/bg.jpg') repeat-y;}
#cnt_L {position:relative;float:left;width:290px;display:inline-block;height:outo;min-height:848px;}
#cnt_R {width:75%;float:left;left:290px;*left:0;margin:20px 0 0 20px;}
#footer {height:50px;background:#e5e5e5;width:100%;min-width:1280px;border-top:solid 1px #d7d7d7;bottom:0;clear:both;}

/*topmenu*/
#header h1 {float:left;width:300px;height:50px;background:url('/img/top_logo.png') no-repeat 0 0;text-indent:-9999px;margin:10px 0 0 10px;}
ul.lnb_menu {position:absolute;padding:10px 20px 0 0;float:right;right:0;}
ul.lnb_menu li {padding:0 2px 0 0;dispaly:inline-block;}
.topmenu {font:bold 13px '돋움';color:#616161;text-decoration:none;cursor:pointer;line-height:30px;background:url(/img/topmenu.png) right 0px no-repeat;
display:block;/*opera*/display:inline-block;/*ie*/display:-moz-inline-box;/*ff*/padding:0px 5px 0px 0px;vertical-align:middle;white-space:nowrap; margin:0px 0px;/*safari*/vertical-align:top;
}
.topmenu span {font:bold 13px '돋움';-moz-user-select:none;*selector-dummy:expression(this.hideFocus=true);text-transform:uppercase;
background:url(/img/topmenu.png) no-repeat left top;height:30px;display:inline-block;padding:0px 20px 0px 25px;line-height:30px;
}
a.topmenu:hover {background-position:right -40px;color:#de7176;margin:0px 0px;/*safari*/}
a.topmenu:hover span {background-position:left -40px;cursor:pointer;}
/* (selected) */
.topmenuSel {color:#e92028;background:url(/img/topmenu.png) no-repeat right -40px;}
.topmenuSel span {background:url(/img/topmenu.png) no-repeat left -40px;}
.topmenuSel:hover span {background:url(/img/topmenu.png) no-repeat left -40px;color:#e92028;}

/*userinfo*/
#user_info {position:relative;height:25px;padding:10px 0 0 10px;background:#333333;min-width:1280px;border-bottom:solid 3px #ec2228;clear:both;color:#cecece;}
.topnotice {float:left;width:500px;}
.block_user {padding:0 20px 0 0;float:right;}
.block_user span {font-weight:bold;}
.block_user img {vertical-align:middle;}


/*footer*/
#footer h1 {float:left;width:300px;height:40px;background:url('/img/bottom_logo.png') no-repeat 0 0;text-indent:-9999px;margin:10px 0 0 10px;}
.copyright {font:normal 11px '돋움';color:#898989;padding:25px 0 0 0;}
.copyright span {font:bold 11px '돋움';color:#ff6565;}

