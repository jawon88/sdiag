<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<%@ taglib prefix="validator" uri="http://www.springmodules.org/tags/commons-validator"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %>   

<%@ page import="java.util.HashMap" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>KT임직원수준진단</title>
<link rel="stylesheet" type="text/css" href="/css/common2.css" />
<script type="text/javascript" src="/js/jquery-1.8.3.min.js"></script>
<script type="text/javascript" src="/js/layer.js"></script>
<script type="text/javaScript" language="javascript">
$(function () {		
	$.noticeView = function(sqno){
		
		$('#sqno').val(sqno);
		document.listForm.action = "<c:url value='/man/noticeview.do'/>";
		document.listForm.submit();
		
	};
   
});
</script>
</head>

<body>
<form:form commandName="searchVO" name="listForm" method="post">
<input type="hidden" name="sqno" id="sqno" value="0" />
</form:form>
<!-- S : layer -->
<div id="ly_mask"></div>
    <!-- S : 상세 내용 및 조치 방안 팝업 -->
    <div id="ly" class="ly1">
        <div class="popup_ly">
            <div class="popup_stt"><li><span>상세 내용 및 조치 방안</span><a href="#"><img src="/img/btn_close.png" alt="close" title="close" /></a></li></div>
            <div class="popup_score">
            	<!-- S : graph block 사이즈 140px -->
                <ul class="popup_graph_block">
                    <li class="cell2"><img src="/img/graph_04.png" /></li>
                    <li class="cell3">score<span>50</span></li>
                    <!-- 소명대상일때 class = calling_1 / 소명배대상일때 class = calling_2 -->
                    <li class="cell1"><span class="calling_2">소명비대상</span></li>                    
                </ul>
                <!-- E : graph block -->
                <div class="popup_cnt">
                	<h4><img src="/img/icon_04.png" /> 상세내용</h4>
                    <li><textarea></textarea></li>
                </div>           
            </div>
            <div class="popup_wrap">
            	<h4><img src="/img/icon_05.png" /> 조치방안</h4>
                <li><textarea></textarea></li>
            </div>
        </div>          
    </div>
    <!-- E : 상세 내용 및 조치 방안 팝업 -->
<!-- E : layer -->

<!-- S : layer -->
<div id="ly_mask2"></div>
    <!-- S : 상세 내용 및 조치 방안 팝업 -->
    <div id="ly" class="ly2">
        <div class="popup_ly">
            <div class="popup_stt"><li><span>상세 내용 및 조치 방안</span><a href="#"><img src="/img/btn_close.png" alt="close" title="close" /></a></li></div>
            <div class="popup_score">
            	<!-- S : graph block 사이즈 140px -->
                <ul class="popup_graph_block">
                    <li class="cell2"><img src="/img/graph_04.png" /></li>
                    <li class="cell3">score<span>50</span></li>
                    <!-- 소명대상일때 class = calling_1 / 소명배대상일때 class = calling_2 -->
                    <li class="cell1"><span class="calling_2">소명비대상</span></li>                    
                </ul>
                <!-- E : graph block -->
                <div class="popup_cnt">
                	<h4><img src="/img/icon_04.png" /> 상세내용</h4>
                    <li><textarea></textarea></li>
                </div>           
            </div>
            <div class="popup_wrap">
            	<h4><img src="/img/icon_05.png" /> 조치방안</h4>
                <li><textarea></textarea></li>
            </div>
        </div>          
    </div>
    <!-- E : 상세 내용 및 조치 방안 팝업 -->
<!-- E : layer -->

<!-- S : header -->
<div id="header">
    <h1>임직원수준진단</h1>
    <div><a href="http://sldm.kt.com/">관리자페이지로 이동</a></div>
</div>
<!-- E : header -->
<div style="clear:both;"></div>
<!-- S : contents -->
<div id="cnt">
	<div id="cnt_left">
    	<div class="user_wrap">
        	<ul class="user_score">
            	<li class="cell1">전사</li>
                <li class="cell2"><img src="/img/dashboard.png" /></li>
                <li class="cell3">55 <span>score</span></li>
            </ul>
            <ul class="user_info">
            	<li class="cell1"><span>홍길동</span>님 환영합니다.</li>
                <li class="cell2"><img src="/img/icon_logout.png" /> <a href="/logout.do">로그아웃</a></li>
            </ul>
        </div>
        <!-- S : notice blcok -->
        <div class="notice_wrap">
        	<h3><img src="/img/icon_02.png" /> <span>notice</span> <a href="/man/noticelist.do"><img src="/img/icon_more.jpg" /> more</a></h3>
        	<ul>
        	<c:forEach var="noticeList" items="${noticeList}" varStatus="status">
				<li><a href="javascript:$.noticeView('${noticeList.sq_no}')" target="_self">${fn:substring(noticeList.title, 0, 18)}<c:if test="${fn:length(noticeList.title) > 18}" >...</c:if></a><span>${noticeList.upd_date}</span></li>
			</c:forEach>
			</ul>
        </div>
        <!-- E : notice blcok -->
        <div class="link_wrap">
        	<li><img src="/img/link_icon_01.png" /> <a href="" target="_blank">한국인터넷 진흥원</a></li>
            <li><img src="/img/link_icon_02.png" /> <a href="" target="_blank">개인정보침해 신고센터</a></li>
            <li><img src="/img/link_icon_03.png" /> <a href="" target="_blank">불법스팸 대응센터</a></li>
            <li><img src="/img/link_icon_04.png" /> <a href="" target="_blank">인터넷침해 대응센터</a></li>
            <li><img src="/img/link_icon_05.png" /> <a href="" target="_blank">주민등록번호 클린센터</a></li>
            <li><img src="/img/link_icon_06.png" /> <a href="" target="_blank">바이러스 토탈</a></li>
        </div>
    </div>    
    <div id="cnt_right">
    	<!-- S : graph block -->
    	<div class="graph_wrap">
        	<ul class="graph_block">
            	<li class="cell1">홍길동</li>
                <li class="cell2"><img src="/img/graph_01.png" /></li>
                <li class="cell3">score<span>75</span></li>
            </ul>
        	<ul class="graph_block">
            	<li class="cell1">IT보안팀</li>
                <li class="cell2"><img src="/img/graph_02.png" /></li>
                <li class="cell3">score<span>25</span></li>
            </ul>
        	<ul class="graph_block">
            	<li class="cell1">IT인프라기획담당</li>
                <li class="cell2"><img src="/img/graph_03.png" /></li>
                <li class="cell3">score<span>50</span></li>
            </ul> 
            <ul class="graph_data">
            	<li><img src="/img/icon_03.png" /><span>데이터 수집날짜 ㅣ</span>Jenuary 25, 2017</li>
            </ul>          
        </div>
        <!-- E : graph block -->
        <!-- S : date block -->
        <div class="date_block">
        	<!-- S : 1줄당 4개 한세트로 복제 -->
            <div class="date_wrap">
            	<!-- S : 복제영역 4개까지만 복제 (*발생사유 없어도 li 영역 유지해야함!)-->
            	<a href="#" class="ly_btn btn_st2" alt="ly1">
                <div>
                    <h4>백신최신보안 패치여부 [80점]</h4>
                    <li class="result_excellent">양호</li>
                    <li class="cause"><span>&nbsp;</span></li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                </div>
                </a>
                <!-- E : 복제영역 4개까지만 복제 -->
                <a href="#" class="ly_btn2 btn_st2" alt="ly2">
                <div>
                    <h4>Windows 자동 로그온 [60점]</h4>
                    <li class="result_care">주의</li>
                    <li class="cause"><span>발생사유</span></li>
                    <li class="cause">- Windows 자동 로그인 설정 </li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                </div>
                </a>
                <div>
                    <h4>엑티브X 취약 [90점]</h4>
                    <li class="result_excellent">양호</li>
                    <li class="cause"><span>&nbsp;</span></li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                </div>
                <div>
                    <h4>백신설치 및 실행여부 [30점]</h4>
                    <li class="result_vulnerable">취약</li>
                    <li class="cause"><span>발생사유</span></li>
                    <li class="cause">- PC내 악성코드 존재</li>
                    <li class="cause">- 백신(V3) 미설치 또는 설정 미흡</li>
                    <li class="cause">&nbsp;</li>
                </div>
            </div>
            <!-- E : 1줄당 4개 한세트로 복제 -->
            <div class="date_wrap">
                <div>
                    <h4>로그인/패스워드 안정성여부 [65점]</h4>
                    <li class="result_care">주의</li>
                    <li class="cause"><span>발생사유</span></li>
                    <li class="cause">- 자동 로그인 설정</li>
                    <li class="cause">- 패스워드 안정성 미흡</li>
                    <li class="cause">&nbsp;</li>
                </div>
                <div>
                    <h4>백신 최신 보안 패치 여부 [20점]</h4>
                    <li class="result_vulnerable">취약</li>
                    <li class="cause"><span>발생사유</span></li>
                    <li class="cause">- 백신 최신 보안 패치 미실행</li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                </div>
                <div>
                    <h4>OS최신보안 패치여부 [90점]</h4>
                    <li class="result_excellent">양호</li>
                    <li class="cause"><span>&nbsp;</span></li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                    <li class="cause">&nbsp;</li>
                </div>
            </div>
        </div>
        <!-- E : date block -->
    </div>
</div>
<!-- E : contents -->
<!-- S : footer -->
<div id="footer"> Copyright ⓒ <span>KT</span> All right reserved. </div>
<!-- E : footer -->
</body>
</html>
